{
  "name": "cimb",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "prepare": "husky install",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "nx": "nx",
    "start": "ng serve --port=4200",
    "start:test": "ng serve --configuration=test",
    "start:uat": "ng serve --configuration=uat",
    "build": "ng build",
    "build:uat": "ng build --configuration=uat",
    "build:dev": "ng build --configuration=development",
    "build:test": "ng build --configuration=test",
    "build:prod": "npm run affected:build --prod --base-href=./",
    "ci": "npm run affected:lint && npm run affected:test && npm run build:prod",
    "test": "ng test --codeCoverage=true",
    "test:ci": "ng test --watch=false --browsers=Headless --source-map=false --codeCoverage=true",
    "test:ci:watch": "ng test --watch=true --source-map=false --codeCoverage=true",
    "test:all": "nx run-many --all --target=test --codeCoverage  --detectOpenHandles --only-failed",
    "lint": "nx workspace-lint && ng lint --detectOpenHandles --only-failed",
    "lint:all": "nx run-many --all --target=lint --detectOpenHandles --only-failed",
    "e2e": "ng e2e",
    "affected:apps": "nx affected --target=apps",
    "affected:libs": "nx affected --target=libs",
    "affected:build": "nx affected --target=build",
    "affected:e2e": "nx affected --target=e2e",
    "affected:test": "nx affected --target=test --parallel=5 --codeCoverage",
    "affected:lint": "nx affected --target=lint --parallel=5",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/environments/",
      "model",
      "environments"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/environments/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/environments/",
      "model",
      "environments"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/environments/",
      "model",
      "(.test)\\.(ts|tsx|js)$",
      "/distribution/.*\\.(ts|js)$"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "^8.0.0",
    "@angular-material-components/moment-adapter": "^8.0.0",
    "@angular/animations": "14.2.8",
    "@angular/cdk": "14.2.6",
    "@angular/common": "14.2.8",
    "@angular/compiler": "14.2.8",
    "@angular/core": "14.2.8",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "14.2.8",
    "@angular/material": "14.2.6",
    "@angular/material-moment-adapter": "14.2.6",
    "@angular/platform-browser": "14.2.8",
    "@angular/platform-browser-dynamic": "14.2.8",
    "@angular/router": "14.2.8",
    "@ng-idle/core": "^11.1.0",
    "@ng-idle/keepalive": "^11.0.3",
    "@ngrx/component-store": "14.0.2",
    "@ngrx/effects": "14.0.2",
    "@ngrx/entity": "14.0.2",
    "@ngrx/router-store": "14.0.2",
    "@ngrx/store": "14.0.2",
    "@nrwl/angular": "15.0.4",
    "angular-google-charts": "^2.2.3",
    "angularx-qrcode": "^14.0.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.2.2",
    "file-saver": "^2.0.5",
    "highcharts": "^9.1.2",
    "highcharts-angular": "^2.10.0",
    "jwt-decode": "^3.1.2",
    "lodash-es": "^4.17.21",
    "modern-normalize": "^1.1.0",
    "moment": "^2.29.2",
    "ngx-pagination": "^5.1.1",
    "rxjs": "~6.6.0",
    "swiper": "^6.8.4",
    "tslib": "^2.0.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.7",
    "@angular-devkit/schematics": "14.2.7",
    "@angular-eslint/eslint-plugin": "14.0.4",
    "@angular-eslint/eslint-plugin-template": "14.0.4",
    "@angular-eslint/template-parser": "14.0.4",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "14.2.8",
    "@angular/language-service": "14.2.8",
    "@ngrx/schematics": "14.0.2",
    "@ngrx/store-devtools": "14.0.2",
    "@nrwl/cli": "15.0.4",
    "@nrwl/cypress": "15.0.4",
    "@nrwl/eslint-plugin-nx": "15.0.4",
    "@nrwl/jest": "15.0.4",
    "@nrwl/linter": "15.0.4",
    "@nrwl/workspace": "15.0.4",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "28.1.8",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "cypress": "7.4.0",
    "dotenv": "10.0.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-sonarjs": "^0.13.0",
    "husky": "^7.0.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "12.2.2",
    "lint-staged": "^11.2.3",
    "nx": "15.0.4",
    "prettier": "2.6.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
